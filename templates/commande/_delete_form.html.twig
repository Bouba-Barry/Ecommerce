<form
  method="post"
  action="{{ path('app_commande_delete', { id: commande.id }) }}"
  onsubmit="return confirm('vous voulez  le supprimer vraiment ??');"
>
  <input
    type="hidden"
    name="_token"
    value="{{ csrf_token('delete' ~ commande.id) }}"
  />
  <button class="btn">Delete</button>
</form>
