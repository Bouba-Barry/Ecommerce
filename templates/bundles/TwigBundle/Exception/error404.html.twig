{% extends 'frontend/index.html.twig' %}

{% block content %}

	<!-- Start Page Banner -->
	<div class="page-banner-area item-bg1">
		<div class="d-table">
			<div class="d-table-cell">
				<div class="container">
					<div class="page-banner-content">
						<h2>Page D'Erreur</h2>
						<ul>
							<li>
								<a href="{{ path('app_home') }}">Accueil</a>
							</li>
							<li>Page D'Erreur</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- End Page Banner -->

	<!-- Start Error Area -->
	<section class="error-area ptb-100">
		<div class="d-table">
			<div class="d-table-cell">
				<div class="container">
					<div class="error-content">
						<img src="{{ asset('FrontOffice/assets/img/E404-2.jpg') }} " alt="error">

						<h3>Erreur 404 : La page n'a pas été trouvé</h3>
						<p>La page que vous recherchez n'est malheureusement pas disponible, Veuillez Retournez à la page précédante ou verifiez le chemin d'accès .</p>

						<a href="{{ path('app_home') }}" class="default-btn">
							Retour à l'accueil
						</a>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End Error Area -->


{% endblock %}
